SELECT FUNCIONARIO.ID_FUNCIONARIO,FUNCIONARIO.NOME FROM FUNCIONARIO , FUNCIONARIO_PROJETO , PROJETO WHERE FUNCIONARIO.ID_FUNCIONARIO=FUNCIONARIO_PROJETO.ID_FUNCIONARIO AND PROJETO.ID_PROJETO=FUNCIONARIO_PROJETO.ID_PROJETO AND FUNCIONARIO.UF='RJ'AND FUNCIONARIO.ANO_NASCIMENTO< 2000 AND PROJETO.DATA_FIM < SYSDATE()  
GROUP BY FUNCIONARIO_PROJETO.ID_FUNCIONARIO
HAVING COUNT(FUNCIONARIO_PROJETO.ID_FUNCIONARIO) >=3;